    "start:remote2": "REACT_APP_ENV=remote2 node scripts/start.js",
    "start:staging": "REACT_APP_ENV=staging node scripts/start.js",
    "start:staging-cy": "NODE_ENV=development BABEL_ENV=development REACT_APP_ENV=staging node scripts/start.js",
    "start:ondeck": "REACT_APP_ENV=ondeck node scripts/start.js",
    "start:neo": "REACT_APP_ENV=neo node scripts/start.js",
    "start:demo": "REACT_APP_ENV=demo node scripts/start.js",
    "start:demo_backup": "REACT_APP_ENV=demo_backup node scripts/start.js",
    "build": "node scripts/build.js",
    "build:cy": "NODE_ENV=development BABEL_ENV=development node scripts/build.js",
    "test": "jest",
    "test:ci": "node scripts/test.js --watchAll=false",
    "get-git-revision": "git rev-parse HEAD",
    "create-info-json": "bash create-info-json.sh",
    "predeploy:alpha": "npm run create-info-json && REACT_APP_ENV=staging npm run build",
    "deploy:alpha": "aws s3 sync build/ s3://ambient.ai-product",
    "postdeploy:alpha": "aws cloudfront create-invalidation --distribution-id E2FNVGZ769J5DP --paths '/*'",
    "predeploy:demo": "npm run create-info-json && REACT_APP_ENV=demo npm run build",
    "deploy:demo": "aws s3 sync build/ s3://ambient.ai-product-dev",
    "postdeploy:demo": "aws cloudfront create-invalidation --distribution-id E26R9LFDGROB4E --paths '/*'",
    "predeploy:demo_backup": "npm run create-info-json && REACT_APP_ENV=demo_backup npm run build",
    "deploy:demo_backup": "aws s3 sync build/ s3://product-20210722220420805500000001",
    "postdeploy:demo_backup": "aws cloudfront create-invalidation --distribution-id E2MD19FAVTW5RR --paths '/*'",
    "predeploy:beta": "npm run create-info-json && REACT_APP_ENV=beta npm run build",
    "deploy:beta": "aws s3 sync build/ s3://ambient.ai-product-beta",
    "postdeploy:beta": "aws cloudfront create-invalidation --distribution-id E1ANFFNVOD0K2M --paths '/*'",
    "predeploy:internal": "node scripts/test.js --watchAll=false --passWithNoTests && npm run create-info-json && REACT_APP_ENV=internal npm run build",
    "deploy:internal": "aws s3 sync build/ s3://ambient.ai-product-internal",
    "postdeploy:internal": "aws cloudfront create-invalidation --distribution-id E8YT500TIS30A --paths '/*'",
    "predeploy:app": "node scripts/test.js --watchAll=false --passWithNoTests && npm run create-info-json && REACT_APP_ENV=app npm run build",
    "deploy:app": "aws s3 sync build/ s3://ambient.ai-product-prod",
    "postdeploy:app": "aws cloudfront create-invalidation --distribution-id E30IAKXBW8T7VB --paths '/*'",
    "predeploy:azure_app": "node scripts/test.js --watchAll=false --passWithNoTests && REACT_APP_ENV=azure_app npm run build",
    "deploy:azure_app": "az storage blob upload-batch -s ./build -d \\$web --account-name ambientwebapp",
    "postdeploy:azure_app": "az cdn endpoint purge -g portal-prod_group --profile-name ambient-web-app --content-paths '/*' --name ambient-web-app --no-wait",
    "predeploy:azure_beta": "REACT_APP_ENV=azure_beta npm run build",
    "deploy:azure_beta": "az storage blob upload-batch -s ./build -d \\$web --account-name ambientwebbeta",
    "postdeploy:azure_beta": "az cdn endpoint purge -g portal-prod_group --profile-name ambient-web-beta --content-paths '/*' --name ambient-web-beta --no-wait",
